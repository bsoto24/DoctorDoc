<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.doctordocplus.doctordoc">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_doctor"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".presenter.activity.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presenter.activity.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presenter.activity.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presenter.activity.PostularActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presenter.activity.ActivacionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presenter.activity.CitasActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presenter.activity.HistorialCitasActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presenter.activity.PreguntasActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presenter.activity.ConfiguracionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presenter.activity.AboutActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presenter.activity.CalificacionActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".presenter.activity.CalendarActivity"></activity>
    </application>

</manifest>